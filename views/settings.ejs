<% include layout-header %>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <h2 class="header">Settings</h2>
    <div class="row">
    <form id="settingsForm" class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <input placeholder="What's in a name?" id="fullName" type="text" class="validate">
          <label for="fullName">Full Name</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input placeholder="City" id="city" type="text" class="validate">
          <label for="city">City</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input placeholder="State" id="state" type="text" class="validate">
          <label for="state">State</label>
        </div>
      </div>
      <div class="row center">
        <a href="#" id="submit" class="btn #b9f6ca green accent-3 waves-effect waves-green">Update</a>
      </div>
    </form>
  </div>
<% include layout-footer %>